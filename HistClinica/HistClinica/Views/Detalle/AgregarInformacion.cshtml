
@{
    ViewData["Title"] = "AgregarInformacion";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>AgregarInformacion</h1>


<hr />
<div class="row">
    <div class="col-md-6">

        <form asp-action="Editar" asp-controller="Detalle" id="frmEnviar">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label class="control-label">Codigo</label>
                <input class="form-control" />
                <span class="text-danger"></span>
            </div>
            <div class="form-group">
                <label class="control-label">Descripcion</label>
                <input class="form-control" />
                <span class="text-danger"></span>
            </div>
            <div class="form-group" align="right">
                <input type="button" value="Guardar" class="btn btn-primary" onclick="Abrir()" />
                <a asp-action="Index" class="btn btn-danger">Regresar</a>
            </div>



        </form>
    </div>
</div>


